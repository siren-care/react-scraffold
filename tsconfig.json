{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"importHelpers": true,
		"jsx": "react",
		"esModuleInterop": true,
		"sourceMap": true,
		"baseUrl": "./",
		"strict": true,
		"paths": {
			"src/*": ["src/*"]
			//   "@@/*": ["src/.umi/*"]
		},
		"allowSyntheticDefaultImports": true
	},
	"include": ["src/**/*", "typings.d.ts"],
	"exclude": ["dist", "node_modules"]
}
